{
  "collectCoverage": true,
  "globals": {
    "__DEV__": true
  },
  "moduleFileExtensions": [
    "js"
  ],
  "preprocessorIgnorePatterns": [
    "node_modules/(?!react-native|apsl-react-native-button)"
  ],
  "preset": "jest-react-native",
  "testRegex": ".*Spec\\.js$",
  "unmockedModulePathPatterns": [
    "react"
  ],
  "verbose": true
}
